    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: AI Doc Chat",
            "type": "debugpy",
            "request": "launch",
            "program": "app.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--reload",
                "--port",
                "9527"
            ],
            "jinja": true,
            "justMyCode": false,
        },
        {
            "name": "Web Connector Debugger",
            "type": "debugpy",
            "request": "launch",
            "module": "core.connector.web"
        }
    ]
}